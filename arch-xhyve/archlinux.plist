<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>biz.giang.archlinux</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true />
    <key>ProgramArguments</key>
    <array>
        <string>/work/.archlinux/boot.sh</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/work/.archlinux</string>

   <key>StandardErrorPath</key>
        <string>/var/log/archlinux.error.log</string>
        <key>StandardOutPath</key>
        <string>/var/log/archlinux.log</string>
  </dict>
</plist>
